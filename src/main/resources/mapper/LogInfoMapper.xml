<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huahuo.huahuobook.mapper.LogInfoMapper">

    <resultMap id="BaseResultMap" type="com.huahuo.huahuobook.pojo.LogInfo">
            <result property="callTime" column="call_time" jdbcType="TIMESTAMP"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="module" column="module" jdbcType="VARCHAR"/>
            <result property="functionName" column="function_name" jdbcType="VARCHAR"/>
            <result property="func" column="func" jdbcType="VARCHAR"/>
            <result property="params" column="params" jdbcType="VARCHAR"/>
            <result property="wasteTime" column="waste_time" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        call_time,user_id,module,
        function_name,func,params,
        waste_time
    </sql>
</mapper>
